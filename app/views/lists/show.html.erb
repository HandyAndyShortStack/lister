<h2><%= @list.name %></h2>
<ul>
  <% @list.items.each do |item| %>
    <li>
      <%= item.name %>
    </li>
  <% end %>
</ul>

<%= link_to "dit List", edit_list_path(@list) %>

<%= link_to "Delete List", list_path(@list), method: :delete %>

<%= link_to "Add New Item", new_list_item_path(@list) %>

